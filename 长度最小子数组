class Solution:
    def minSubArrayLen(self, target: int, nums: List[int]) -> int:
        left, right = 0, 0
        res = 0
        length = float('inf')
        while right < len(nums):
            res += nums[right]
            while res >= target:
                length = min(length, right - left + 1)
                res -= nums[left]
                left += 1
            right += 1
        if length != float('inf'):
            return length
        else:
            return 0
